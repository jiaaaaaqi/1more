 <script src="https://static0.1more.com/s/js/html5media.min.js"></script>
<script>
    function play() {
        var play = document.getElementById("audio");
        $(".pause").css('display','block');
        $(".play").css('display','none');
        play.play();
    }

    function pause() {
        var play = document.getElementById("audio");
        $(".pause").css('display','none');
        $(".play").css('display','block');
        play.pause();
    }
</script>
   
