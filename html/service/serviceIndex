<script>
		(function($) { 
			$.widget("custom.combobox", {
				_create: function() {
					this.wrapper = $("<span>")
						.addClass("custom-combobox")
						.insertAfter(this.element); 
					
					this.element.hide(); 
					this._createAutocomplete();
					this._createShowAllButton();
				},

				_createAutocomplete: function() {
					var selected = this.element.children(":selected"),
						value = selected.val() ? selected.text() : "";

					this.input = $("<input name='productCode_id' onfocus='flushText(this)'>")
						.appendTo(this.wrapper)
						.val(value)
						.attr("title", "")
						.addClass("custom-combobox-input ui-widget ui-widget-content ui-state-default ui-corner-left")
						.autocomplete({
							delay: 0,
							minLength: 0,
							source: $.proxy(this, "_source"),
							//选中事件执行
							select: function(event, ui) {
								//  getProductStation();
							}
						})
						.tooltip({
							tooltipClass: "ui-state-highlight"
						});

					this._on(this.input, {
						autocompleteselect: function(event, ui) {
							ui.item.option.selected = true;
							this._trigger("select", event, {
								item: ui.item.option
							});
						},

						autocompletechange: "_removeIfInvalid"
					});
				},

				_createShowAllButton: function() {
					var input = this.input,
						wasOpen = false;

					$("<a>")
						.attr("tabIndex", -1)
						.attr("title", "显示全部选项")
						.tooltip()
						.appendTo(this.wrapper)
						.button({
							icons: {
								primary: "ui-icon-triangle-1-s"
							},
							text: false
						})
						.removeClass("ui-corner-all")
						.addClass("custom-combobox-toggle ui-corner-right")
						.mousedown(function() {
							wasOpen = input.autocomplete("widget").is(":visible");
						})
						.click(function() {
							input.focus();

							// 如果已经可见则关闭
							if(wasOpen) {
								return;
							}

							// 传递空字符串作为搜索的值，显示所有的结果
							input.autocomplete("search", "");
						});
				},

				_source: function(request, response) {
					var matcher = new RegExp($.ui.autocomplete.escapeRegex(request.term), "i");
					response(this.element.children("option").map(function() {
						var text = $(this).text();
						if(this.value && (!request.term || matcher.test(text)))
							return {
								label: text,
								value: text,
								product: this.value,
								option: this
							};
					}));
				},

				_removeIfInvalid: function(event, ui) {
					
					
						// 选择一项，不执行其他动作
						if(ui.item) {
							return;
						}
	
						// 搜索一个匹配（不区分大小写）
						var value = this.input.val(),
							valueLowerCase = value.toLowerCase(),
							valid = false;
						this.element.children("option").each(function() {
							if($(this).text().toLowerCase() === valueLowerCase) {
								this.selected = valid = true;
								//输入一致后执行执行
								//getProductStation();
								return false;
							}
						});
	
						// 找到一个匹配，不执行其他动作
						if(valid) {
							return;
						}
						
						// 移除无效的值
						// this.input.val( "" ).attr( "title", " 没有找到匹配" +value +"的选项" ).tooltip( "open" );
						if(value!=""){
							alert("没有找到匹配:" + value + "的选项");
						}
						var Element_selectId = $(this.input).parent().prev().attr("id");
						if(Element_selectId=="repairProductCode"){
							this.input.val("请选择产品");
							$("#CompentName").next().children().val("选择产品后才可选择返修点");
						}else{
							this.input.val("请选择返修点");
						}
						
						this.element.val("");
						
						/*this._delay(function() {
						  this.input.tooltip( "close" ).attr( "title", "" );
						}, 2500 );*/
						this.input.data("ui-autocomplete").term = "";
						// clearAll();//清除内容 
				
					
					},

				_destroy: function() {
					this.wrapper.remove();
					this.element.show();
				}
			});
		})(jQuery);
		
	

	function refreshImage(img){
	    var imageUrl = '/service/getValidImage.do';  
	    img.src = imageUrl + '?iddd=' + Math.random();
	}
	
	function getResult(){
		if(validate()){
			return;
		};
		
		var id = $("#CompentName :selected").val();
		var imgcode = $("#Verification").val();
		refreshImage(document.getElementById('validateimg'));
		$("#Verification").val("");
		$.ajax({
			data:{'nPId':id,'imageCode':imgcode},
			dataType:"json",
			url:"/service/getResovleResult.do",
			type:"post",
			success:function(data){
				if(data.code==0){
					var productTitle =$("#repairProductCode option:selected").text();
					var productCode = productTitle.split("-")[0];
					$("#productResult").html(productCode);
					$("#compentNameResult").html(data.strCompentName);
					$("#resovleResult").html(data.strResovle);
					$("#priceResult").html(data.nPrice.substring(0,data.nPrice.length-2)+"元");
					$("#tips_result").show();
					$("#tips_error").css("display","none");
					
				}else if(data==-1){
					alert("未查到方案价格");
					imgcode.val("");

				}else if(data==-2){
					$("#tips_error").css("display","inline");

				}
				
			}
			
		});
	}
	

	
	function validate(){
		var repairProductCode = $("#repairProductCode").next().children().val();
		var CompentName = $("#CompentName").next().children().val();
		if(repairProductCode==null||repairProductCode==""||repairProductCode=="请选择产品"){
			alert("请选择产品");
			return true;
		}
		if(CompentName==null||CompentName==""||CompentName=="选择产品后才可选择返修点"||CompentName=="请选择返修点"){
			alert("请选择返修点");
			return true;
		}
	}
	
	function flushText (e){
			var id = $(e).parent().prev().attr("id");
			if(id=="repairProductCode"){
				$("#repairProductCode").next().children().val("");
			}else{
				$("#CompentName").next().children().val("");
			}
			
		}
	
	$(function(){
		
		
		$("#repairProductCode").combobox({
				
				select: function(event, ui){
					$("#init_tips").remove();
					
					$("#tips_result").hide();
					code = $("#repairProductCode :selected").val();
					$.ajax({
						data:{'strProductModelCode':code},
						dataType:"json",
						url:"/service/getCompentNamebyModelCode.do",
						type:"post",
						success:function(data){
							if(data!=-1){
								$("#CompentName").next().children().val("请选择返修点");
								
								$("#CompentName").html("<option></option>");
								$.each(data, function(index,value) {
									$("#CompentName").append("<option value='"+value.nPId+"'>"+value.strCompentName+"</option>");
								});
							}else{
								$("#repairProductCode").next().children().blur();
								$("#CompentName").next().children().val("选择产品后才可选择返修点");
								$("#CompentName").html("");
								alert("该产品还未配置返修点");
								
									$("#repairProductCode").next().children().val("请选择产品");
							} 
						}
					});
				}
			});
		$("#CompentName").combobox({
			select: function(){
				$("#tips_result").hide();
			}
			
		});
	})
	
</script>

